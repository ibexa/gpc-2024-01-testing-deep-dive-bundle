{
  "name": "ibexa/gpc-2024-01-testing-deep-dive-bundle",
  "license": "proprietary",
  "type": "symfony-bundle",
  "scripts": {
    "phpstan": "phpstan",
    "test": "phpunit"
  },
  "require": {
    "php": "^8.1",
    "ibexa/admin-ui": "~4.6.x-dev",
    "ibexa/core": "~4.6.x-dev",
    "ibexa/rest": "~4.6.x-dev",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/event-dispatcher-contracts": "^2.2",
    "symfony/http-foundation": "^5.4",
    "symfony/http-kernel": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "ibexa/test-core": "0.1.x-dev",
    "ibexa/test-rest": "0.1.x-dev",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^9",
    "symfony/event-dispatcher": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Ibexa\\Gpc202401TestingDeepDiveBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ibexa\\Gpc202401TestingDeepDiveBundle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": false,
    "sort-packages": true
  }
}
